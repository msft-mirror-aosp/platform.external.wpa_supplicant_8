filegroup {
    name: "wpa_supplicant_template.conf",
    srcs: ["wpa_supplicant_template.conf"],
}

cc_defaults {
    name: "wpa_supplicant_defaults",
    defaults: ["wpa_supplicant_cflags_defaults"],
    srcs: [":wpa_supplicant_srcs"],
